{{!
    @template local_booking/logentry_summary_body

    This template renders the body of calendar logentrys summary modal.
}}
<div{{!
    }} class="summary-modal-container"{{!
    }} data-region="summary-modal-container"{{!
    }} data-type="logentry"{{!
    }} data-logentry-id="{{id}}"{{!
    }} data-flight-date="{{flightdate}}"{{!
    }} data-flight-type="{{flighttype}}"{{!
    }} data-find-pirep="{{hasfindpirep}}"{{!
    }} data-user-id="{{userid}}"{{!
    }} data-exercise-id="{{exerciseid}}"{{!
    }} data-session-id="{{sessionid}}"{{!
    }} data-course-id="{{courseid}}"{{!
    }} data-logentry-title="{{#str}} logentry, local_booking {{/str}}"{{!
    }}>
    <div class="container-fluid">
        {{> local_booking/logentry_summary_details }}
    </div>
</div>
