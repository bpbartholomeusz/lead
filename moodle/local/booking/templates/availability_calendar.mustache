{{!
    @template availability/availability_calendar

    Calendar week view for availability posting.

    The purpose of this template is to render the main, detailed weekly view.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
    }
}}

<div id="calendar-week-{{uniqid}}" data-template="local_booking/availability_week">
    {{> local_booking/availability_week}}
</div>

{{#js}}
require(['jquery', 'local_booking/calendar'], function($, Calendar) {
    Calendar.init($("#calendar-week-{{uniqid}}"));
});
{{/js}}
